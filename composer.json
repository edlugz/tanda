{
    "name": "edlugz/tanda",
    "description": "This package provides you with the means to make requests to the Tanda API",
    "license": "MIT",
    "authors": [
        {
            "name": "Eddy Lugaye",
            "email": "eddy.lugaye@gmail.com"
        }
    ],
    "homepage": "https://github.com/edlugz/tanda",
    "keywords": ["Laravel", "Tanda"],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^v8.15.0"
    },
    "autoload": {
        "psr-4": {
            "EdLugz\\Tanda\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdLugz\\Tanda\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EdLugz\\Tanda\\TandaServiceProvider"
            ],
            "aliases": {
                "Tanda": "EdLugz\\Tanda\\Facades\\Tanda"
            }
        }
    }
}
